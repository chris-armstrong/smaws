(rule
 (mode promote)
 (deps
  (:gen ../../bin/AwsGenerator.exe)
  (:input ../../definitions/shield.json))
 (targets
  builders.ml
  builders.mli
  serializers.ml
  deserializers.ml
  types.ml
  types.mli
  service_metadata.ml
  service_metadata.mli
  operations.ml
  operations.mli
  Smaws_Client_Shield.ml
  Smaws_Client_Shield.mli)
 (action
  (run
   %{gen}
   -input
   %{input}
   -output
   .
   -targets
   builders
   types
   service_metadata
   serializers
   deserializers
   operations
   module)))

(library
 (name Smaws_Client_Shield)
 (public_name smaws-clients.shield)
 (modules
  Smaws_Client_Shield
  types
  service_metadata
  serializers
  deserializers
  operations
  builders)
 (libraries smaws-lib))
